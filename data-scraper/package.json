{
  "name": "audio-data-scraper",
  "version": "1.0.0",
  "description": "Scraper for downloading stereo audio training data from Freesound, Internet Archive, and Musopen",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "scrape:freesound": "ts-node src/index.ts --source freesound",
    "scrape:archive": "ts-node src/index.ts --source archive",
    "scrape:musopen": "ts-node src/index.ts --source musopen",
    "scrape:all": "ts-node src/index.ts --source all"
  },
  "keywords": [
    "audio",
    "scraper",
    "machine-learning",
    "dataset"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "music-metadata": "^9.0.0",
    "open": "^11.0.0",
    "p-queue": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
